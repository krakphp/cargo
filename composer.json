{
    "name": "krak/cargo",
    "description": "Small yet Powerful container library inspired by pimple",
    "type": "library",
    "license": "MIT",
    "keywords": [
       "container",
       "service",
       "di",
       "pimple",
       "container-interop"
   ],
    "authors": [
        {
            "name": "RJ Garcia",
            "email": "rj@bighead.net"
        }
    ],
    "require-dev": {
        "evert/phpdoc-md": "^0.2.0",
        "krak/auto-args": "^0.3.1",
        "lavoiesl/php-benchmark": "^1.4",
        "peridot-php/leo": "^1.6",
        "peridot-php/peridot": "^1.18",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Krak\\Cargo\\": "src"
        },
        "files": ["src/Container/options.php", "src/cargo.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Krak\\Cargo\\Test\\": "test"
        }
    },
    "require": {
        "psr/container": "^1.0",
        "psr/log": "^1.0"
    }
}
